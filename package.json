{
  "name": "tsbot",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "bun run format && cross-env NODE_ENV=production bun .",
    "dev": "bun run format && cross-env NODE_ENV=development bun .",
    "format": "biome check --apply --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true ./src",
    "clean": "rimraf dist",
    "build": "bun run format && bun run clean && tsc",
    "prepare": "husky"
  },
  "packageManager": "bun@1.1.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "glob": "^10.3.12",
    "husky": "^9.0.11",
    "mongoose": "^8.3.1",
    "undici": "^6.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.4"
  }
}